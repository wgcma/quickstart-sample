    "@react-native-community/cli": "latest",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^9.31.0",
    "eslint-config-expo": "^9.2.0",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.9",
